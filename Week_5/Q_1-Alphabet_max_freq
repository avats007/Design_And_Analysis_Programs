

#include <iostream>
#include <algorithm>                    // Header-file under which sort works
#include<unordered_map>
#include<vector>
using namespace std;

int main(){
    unordered_map<char,int> m;
    pair<char,int> p('a',0);
    
    int n,i;
    cout<<"Enter the size of the array\n";
    cin>>n;
    
    char arr[n];
    
    cout<<"Enter the elements of the array\n";
    
    for(i=0;i<n;i++)
    cin>>arr[i];
    
    for(i=0;i<n;i++){
        
        if(m.find(arr[i])!=m.end()){
            m[arr[i]]++;
        }
        
        m[arr[i]]=1;
    }

   for(auto it = m.begin(); it!=m.end(); it++)
   {
        if(it->second>p.second){
        p.second= it->second;
        p.first = it->first;
        }
   }
   
   cout<<"Alphabet with maximum number of occurrences are: "<<p.first;
    
    return 0;
}
